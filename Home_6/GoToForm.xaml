<Window x:Class="Home_6.GoToForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Home_6"
        mc:Ignorable="d"
        Title="Go To Line" Height="140" Icon="Icons/Notepad.ico" Width="270" ResizeMode="NoResize" Loaded="Window_Loaded">
    
    <StackPanel>
        <Label Margin="5" Name="lMaxLine" />
        <TextBox Margin="3" Name="txtResul" />
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
            <Button Name="btnOK" Width="70" Margin="5" Click="btnOK_Click">OK</Button>
            <Button Width="70" Margin="5" Click="Button_Click">Cancel</Button>
        </StackPanel>
    </StackPanel>
</Window>